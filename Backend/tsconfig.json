{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "server",
    "strict": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true /* Allow default imports from modules with no default export. This does not affect code emit, just typechecking. */,
    "esModuleInterop": true,
    "moduleResolution": "node"
  },
  "include": ["server/**/*"]
}
